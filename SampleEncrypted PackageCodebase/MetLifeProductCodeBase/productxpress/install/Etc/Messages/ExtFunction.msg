<?xml version = "1.0" ?>
<MessageModule name = "ExtFunction">
	<Message id = "EXT_FUNC_XML"
	    message = "External functions xml file contained error. See the error log for details."
		   type = "user" />
	<Message id = "EXT_FUNC_UNKNOWN"
	    message = "Unknown External Function: '%1'."
		   type = "user"/>
	<Message id = "EXT_FUNC_TARGET_UNKNOWN"
	    message = "Unknown Target '%1' for External Function '%2'."
		   type = "user" />
	<Message id = "EXT_FUNC_RET"
	    message = "External function '%1' in '%2' with arguments '%3' returned '%4'."
		   type = "user"/>
	<Message id = "EXT_FUNC_DLL"
	    message = "External function '%1' could not be loaded from dll '%2'."
		   type = "user"/>
	<Message id = "EXT_FUNC_NO_TARGET"
	    message = "External function '%1' target undefined."
		   type = "user"/>
	<Message id = "DELEGATE_CALL_OBJECT"
	    message = "Incorrect named object configuration for delegate call in function '%1'." />
	<Message id = "NO_SUCH_TUPLE_MEMBER"
		message = "No member with name %1 defined in tuple '%2'"
	       type = "user"	/>
	<Message id = "INVALID_DATA_TYPE"
		message = "Invalid data type %1"
	       type = "user"	/>
	<Message id = "INCOMPATIBLE_DATA_STRUCTURE"
		message = "Incompatible Data Structure expecting '%1', found '%2'"
		type = "user"	/>
	<Message id = "NO_TARGET_SET"
		message = "No target set for External Funcion '%1'"
		type = "user"	/>
	<Message id = "XF_NAME_MISSING"
		message = "Name attribute required for external function in ExternalFunctions.Settings"
		type = "user"	/>
	<Message id = "XF_MULTPLY_DEFINED_TARGET"
		message = "Multiply defined target '%1' for external function '%2' in ExternalFunctions.Settings"
		type = "user"	/>
	<Message id = "XF_NO_TARGET"
		message = "No target defined for external function '%1' in ExternalFunctions.Settings"
		type = "user"	/>
	<Message id = "XF_NO_PROPNAME"
		message = "Missing name attribute for Property in ExternalFunctions.Settings"
		type = "user"	/>
	<Message id = "XF_NO_DLL"
		message = "Missing file attribute for target '%1' function '%2' in ExternalFunctions.Settings"
		type = "user"	/>
	<Message id = "XF_NO_ENTRY_POINT"
		message = "Missing entry-point attribute for target '%1' function '%2' in ExternalFunctions.Settings"
		type = "user"	/>
	<Message id = "XF_MULTPLY_DEFINED_PROPERTY"
		message = "Multiply defined Property '%1' for target '%2' function '%3' in ExternalFunctions.Settings"
		type = "user"	/>
	<Message id = "XF_MULTPLY_DEFINED_FUNCTION"
		message = "Multiply defined External Function '%1' in ExternalFunctions.Settings"
		type = "user"	/>
	<Message id = "NO_RETURN_TYPE"
		message = "External function '%1' is called with dangling or null ValueDefinition as return-type"
		type = "user"	/>
	<Message id = "EXTERNAL_FUNCTION_ERROR"
		message = "Error external function '%1' returned with errors: %2"
		type = "user"	/>
	<Message id = "UNDEFINED_EXTERNAL_FUNCTION"
		message = "External function '%1' not found."
		type = "user"	/>

</MessageModule>

